{
  "name": "nuxt-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "test": "jest --clearCache && jest --no-cache",
    "precommit": "npm run lint",
    "stylelint": "stylelint ./pages/**/*.vue --fix"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.4",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/sentry": "^4.5.0",
    "@toast-ui/vue-editor": "^2.5.1",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.17.3",
    "cookie": "^0.4.1",
    "cookie-universal-nuxt": "^2.1.4",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "js-cookie": "^2.2.1",
    "marked": "^2.0.3",
    "moment": "^2.29.1",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "nuxt": "^2.14.6",
    "vue": "^2.6.12",
    "vue-monaco": "^1.2.2",
    "vue-toasted": "^1.1.28",
    "vuex-persistedstate": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.11",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vue/server-test-utils": "^1.1.2",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-transform-dynamic-import": "^2.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.3.6",
    "jest": "^24.1.0",
    "jest-canvas-mock": "^2.2.0",
    "jest-raw-loader": "^1.0.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "stylelint": "^13.11.0",
    "stylelint-config-standard": "^20.0.0",
    "vue-jest": "^4.0.0-0",
    "webpack": "^4.46.0"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleFileExtensions": [
      "js",
      "vue",
      "json"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      ".*\\.(vue)$": "vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/$1",
      "^vue$": "vue/dist/vue.common.js",
      "\\.(s?css|less)$": "identity-obj-proxy",
      "\\.(txt)": "<rootDir>/static/templates/python.txt"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**",
      "!**/.nuxt/**",
      "!**/coverage/**",
      "!nuxt.config.js",
      "!babel.config.js",
      "!theme/**",
      "!plugins/**",
      "!api/**",
      "!layouts/**",
      "!nuxt.config.docker.js",
      "!stylelint.config.js"
    ],
    "coverageReporters": [
      "html",
      "text"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!font-awesome)"
    ]
  }
}
